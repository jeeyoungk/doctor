  "dependencies": {
    "commander": "^14.0.0",
    "semver": "^7.7.2",
    "zod": "^4.0.0",
    "zx": "^8.8.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@evilmartians/lefthook": "^1.9.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.4",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node": "^22.5.1",
    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsonc": "^2.20.1",
    "globals": "^16.3.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.2.0",
    "semantic-release": "^24.2.7",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typedoc": "^0.27.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.39.1",
    "vitest": "^2.0.5"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://www.npmjs.com/package/@jeeyoungk/doctor",
  "keywords": [],
  "license": "UNLICENSED",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "doctor": "./dist/doctor.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "name": "@jeeyoungk/doctor",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeeyoungk/doctor.git"
  },
  "scripts": {
    "build": "tsup",
    "check": "bun run check:format && bun run check:lint && bun run check:attw",
    "check:attw": "attw --pack .",
    "check:format": "prettier --check .",
    "check:lint": "eslint .",
    "clean": "rm -rf dist docs",
    "dist": "bun run clean && bun run build",
    "docs": "typedoc",
    "fix": "bun run fix:format && bun run fix:lint",
    "fix:format": "prettier --write .",
    "fix:lint": "eslint . --fix",
    "release": "semantic-release",
    "test": "bun run test:type && bun run test:unit",
    "test:type": "tsc",
    "test:unit": "vitest run --passWithNoTests"
  },
  "type": "module",
  "version": "1.0.2",
  "packageManager": "bun@1.1.34",
  "engines": {
    "node": ">=18.0.0"
  }
}
